    "source": {
        "directories": [
            "src/RunOpenCode"
        ],
        "excludes": [
            "{src/RunOpenCode/Bundle/*/Tests/*}",
            "{src/RunOpenCode/Component/*/Tests/*}"
        ]
    },
    "logs": {
        "text": "build/infections/report.txt",
