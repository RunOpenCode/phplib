{
    "name": "runopencode/query",
    "description": "Write your dynamic queries in separate files, execute them in your repository.",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikola Svitlica a.k.a TheCelavi",
            "email": "thecelavi@runopencode.com"
        }
    ],
    "require": {
        "php": ">=8.4",
        "doctrine/dbal": "^4.0",
        "twig/twig": "^3.0",
        "psr/cache": "^3.0",
        "symfony/cache": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "RunOpenCode\\Component\\Query\\": "src"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../Component/Dataset",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../../Component/Metadata",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../../Bundle/MetadataBundle",
            "options": {
                "symlink": false
            }
        }
    ],
    "version": "0.2",
    "minimum-stability": "dev"
}
