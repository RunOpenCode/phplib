{
    "name": "runopencode/metadata",
    "description": "Utility to read/search for class/property/method metadata (attributes).",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikola Svitlica a.k.a TheCelavi",
            "email": "thecelavi@runopencode.com"
        }
    ],
    "require": {
        "php": ">=8.4",
        "psr/cache": "^3.0",
        "symfony/cache": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "RunOpenCode\\Component\\Metadata\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../Component/Dataset",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../../Component/Query",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../../Bundle/MetadataBundle",
            "options": {
                "symlink": false
            }
        }
    ],
    "version": "0.2",
    "minimum-stability": "dev"
}
