{
    "name": "runopencode/metadata-bundle",
    "description": "Symfony integration with runopencode/metadata component.",
    "minimum-stability": "stable",
    "license": "proprietary",
    "version": "0.2",
    "authors": [
        {
            "name": "Nikola Svitlica a.k.a TheCelavi",
            "email": "thecelavi@runopencode.com"
        }
    ],
    "require": {
        "runopencode/metadata": "^0.2",
        "php": ">=8.4",
        "psr/cache": "^3.0",
        "symfony/http-kernel": "^7.0",
        "symfony/cache": "^7.0",
        "symfony/config": "^7.0",
        "symfony/dependency-injection": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "RunOpenCode\\Bundle\\MetadataBundle\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../Component/Metadata",
            "options": {
                "symlink": false
            }
        }
    ]
}
